				Name="VCPostBuildEventTool"
				Description="Registering Server"
				CommandLine="regsvr32 /s $(TargetDir)&#1040;&#1080;Odbc.dll
copy debug\OdbcFb32.dll c:\windows\system32\OdbcFb32.dll

"/>
